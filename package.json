{
  "name": "@ember/legacy-built-in-components",
  "version": "0.4.0",
  "description": "Provides the legacy implementation of the `Checkbox`, `LinkComponent`, `TextArea` and `TextField` component classes as specified in RFC #671.",
  "keywords": [
    "ember-addon"
  ],
  "homepage": "https://github.com/emberjs/ember-legacy-built-in-components",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emberjs/ember-legacy-built-in-components.git"
  },
  "bugs": {
    "url": "https://github.com/emberjs/ember-legacy-built-in-components/issues"
  },
  "license": "MIT",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepack": "ember ts:precompile",
    "postpack": "ember ts:clean"
  },
  "dependencies": {
    "@embroider/macros": "^1.0.0",
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-cli-typescript": "^4.1.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
